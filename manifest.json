{
	"name": "Sergei's Test Extension",
	"version": "1.0",
	"manifest_version": 2,

	"description": "Sergei Test",
	"icons": {
		"48": "icon_48.png"
	},
	"browser_action": {
		"default_popup": "popup.html",
		"default_title": "Sergei"
	},
	"content_scripts": [ {
	"js": [ "js/lib/jquery-1.8.2.min.js", "js/app/popup.js" ],
	"matches": [ "*://*/*" ],
	"run_at": "document_start"
	} ],
	"minimum_chrome_version": "18",
	"permissions": [ "http://*/*", "https://*/*", "unlimitedStorage", "contextMenus", "cookies", "tabs", "notifications" ]

}